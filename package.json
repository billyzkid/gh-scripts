{
  "name": "gh-scripts",
  "version": "1.0.0-alpha.14",
  "license": "MIT",
  "bin": {
    "gh-scripts": "./bin/gh-scripts.mjs"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0",
    "yargs": "^15.4.1"
  },
  "scripts": {
    "authors-help1": "node ./bin/gh-scripts.mjs authors --help",
    "authors-help2": "node ./bin/gh-scripts.mjs authors -h",
    "authors-help3": "node ./bin/gh-scripts.mjs authors -?",
    "authors-version1": "node ./bin/gh-scripts.mjs authors --version",
    "authors-version2": "node ./bin/gh-scripts.mjs authors -v",
    "authors-example1": "node ./bin/gh-scripts.mjs authors",
    "authors-example2": "node ./bin/gh-scripts.mjs authors -r HEAD -x robot -f ./AUTHORS -e utf8",
    "changelog-help1": "node ./bin/gh-scripts.mjs changelog --help",
    "changelog-help2": "node ./bin/gh-scripts.mjs changelog -h",
    "changelog-help3": "node ./bin/gh-scripts.mjs changelog -?",
    "changelog-version1": "node ./bin/gh-scripts.mjs changelog --version",
    "changelog-version2": "node ./bin/gh-scripts.mjs changelog -v",
    "changelog-example1": "node ./bin/gh-scripts.mjs changelog",
    "changelog-example2": "node ./bin/gh-scripts.mjs changelog -r HEAD -x robot -f ./AUTHORS -e utf8"
  }
}
